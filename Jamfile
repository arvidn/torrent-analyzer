import modules ;

BOOST_ROOT = [ modules.peek : BOOST_ROOT ] ;

use-project /torrent : ../libtorrent ;

if $(BOOST_ROOT)
{
	use-project /boost : $(BOOST_ROOT) ;
}

exe torrent_analyzer : torrent_analyzer.cpp
	: <threading>multi
	<library>/torrent//torrent
	<library>/boost//filesystem
	: <link>static
	;

